<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.edu.niit.jobrecruitment.model.JobApplication"
		table="jobApplication">
		<id name="id" type="int">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="userId" type="int">
			<column name="userId" />
		</property>
		<property name="enterpriseId" type="int">
			<column name="enterpriseId" />
		</property>
		<property name="postsId" type="int">
			<column name="postsId" />
		</property>
		<property name="resumeId" type="int">
			<column name="resumeId" />
		</property>
		<property name="status" type="int">
			<column name="status" />
		</property>
		<property name="createDate" type="date" update="false">
			<column name="createDate" />
		</property>
	</class>
</hibernate-mapping>
