<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.edu.niit.jobrecruitment.model.Enterprise" table="enterprise">
        <id name="id" type="int">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="userName" type="string">
            <column name="userName" />
        </property>
        <property name="password" type="string">
            <column name="password" />
        </property>
        <property name="email" type="string">
            <column name="email" />
        </property>
        <property name="compName" type="string">
            <column name="compName" />
        </property>
        <property name="industry" type="string">
            <column name="industry" />
        </property>
        <property name="corptype" type="int">
            <column name="corptype" />
        </property>
        <property name="corpsize" type="int">
            <column name="corpsize" />
        </property>
        <property name="compAddress" type="string">
            <column name="compAddress" />
        </property>
        <property name="companyPersin" type="string">
            <column name="companyPersin" />
        </property>
        <property name="contactor" type="string">
            <column name="contactor" />
        </property>
        <property name="sex" type="boolean">
            <column name="sex" />
        </property>
        <property name="phone" type="string">
            <column name="phone" />
        </property>
        <property name="licenseImage" type="string">
            <column name="licenseImage" />
        </property>
        <property name="authenticate" type="int">
            <column name="authenticate" />
        </property>
        <property name="regDate" type="date" update="false">
            <column name="regDate" />
        </property>
    </class>
</hibernate-mapping>